#!/bin/sh
#
# Homebrew Casks
#
# This installs the apps I use via Homebrew casks

if test ! $(which brew)
then
  /bin/sh $ZSH/homebrew/install.sh.osx
fi

brew install phinze/cask/brew-cask

function installcask() {
	brew cask install "${@}" 2> /dev/null
}

installcask adium
installcask alfred
installcask caffeine
installcask dropbox
installcask firefox
installcask google-chrome
installcask iterm2
installcask the-unarchiver
installcask vlc
